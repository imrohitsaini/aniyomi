<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PrimaryButton">
        <attr name="buttonType" format="enum">
            <enum name="positive" value="0" />
            <enum name="negative" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ChipView">
        <attr name="chipText" format="string" />
        <attr name="chipIcon" format="reference" />
        <attr name="chipSelected" format="boolean" />
        <attr name="chipType" format="enum">
            <enum name="chip_button" value="0" />
            <enum name="filter" value="1" />
        </attr>
    </declare-styleable>
</resources>
